/*======================================*/

.target_block {
	/* 한 줄 자르기 */
	display: block;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

.target_inline {
	/* 유동적 너비 자르기 */
	display: inline-block;
	width: 200px;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

.target_box {
	/* 여러 줄 자르기 */
	display: inline-block;
	max-width: 8em;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: normal;
	line-height: 1.15em;
	height: 3.6em;
	text-align: left;
	word-wrap: break-word;
	display: -webkit-box;
	-webkit-line-clamp: 4;
	-webkit-box-orient: vertical;
}

/*======================================*/
/* overlay */
.overlay,
.overlayWhite,
.overlayBlack {
	position: absolute;
	top: 0;
	right: 0;
	bottom: 0;
	width: 100%;
	height: 100%;
	z-index: 9999999;
}

.overlayWhite_bg {
	position: absolute;
	left: 0;
	right: 0;
	bottom: 0;
	width: 100%;
	height: 100%;
	z-index: 999999;
	background-color: #fff;
	opacity: 0.4;
}

.overlayBlack_bg {
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	width: 100%;
	height: 100%;
	z-index: 999999;
	background-color: #222;
	opacity: 0.7;
}
/* overlay-Loading */
.overlayWhite .loading {
	position: relative;
	width: 100%;
	height: 100%;
	z-index: 99999999;
}

/*======================================*/
/* Button  */
button {
	-moz-appearance: none;
	-webkit-appearance: none;
	-ms-appearance: none;
	appearance: none;
	-moz-transition: background-color 0.2s ease-in-out, color 0.2s ease-in-out, box-shadow 0.2s ease-in-out;
	-webkit-transition: background-color 0.2s ease-in-out, color 0.2s ease-in-out, box-shadow 0.2s ease-in-out;
	-ms-transition: background-color 0.2s ease-in-out, color 0.2s ease-in-out, box-shadow 0.2s ease-in-out;
	transition: background-color 0.2s ease-in-out, color 0.2s ease-in-out, box-shadow 0.2s ease-in-out;
	text-decoration: none;
	box-shadow: none;
	cursor: pointer;
	position: relative;
	display: inline-block;
	background: transparent;
	border: 0;
	border-radius: 0;
	font-size: revert;
	font-weight: 600;
	text-align: center;
	padding: 0.5em 1.5em;
	font-family: inherit;
	min-height: 2.625em;
}

ul.button_group {
	display: flex;
	justify-content: space-between;
	gap: 0.625em;
	table-layout: fixed;
	word-break: break-all;
	width: 100%;
}

	ul.button_group > li {
		display: table-cell;
		flex: 1 1;
	}

	ul.button_group a {
		width: 100%;
	}

	ul.button_group a li.choose:active {
		outline:2px solid #FFCC0C;
		border:0;
	}

.button {
	width: 100%;
	text-align: center;
	background: #fff;
	height: 100%;
	font-size: 0.87em;
	margin: 0 auto;
	padding: 0.97em 0;
	border:1px solid #333;
	border-radius: 0.25em;
}

.special {
	background-color: #FFCC0C !important;
	color: #222;
	border:0;
}


ul.actions.fit {
	display: table;
	margin: 0;
	padding: 0;
	table-layout: fixed;
	word-break: break-all;
	width: 100%;
}
	ul.actions.fit > li {
		display: table-cell;
		padding: 0;
	}
		ul.actions.fit > li > * {
			margin-bottom: 0;
			vertical-align: middle;
		}
