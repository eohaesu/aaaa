}
.agree .pop-list .list-tit {
    font-weight: 600;
    margin-bottom: 0;
}
.agree .pop-list>li:first-child {
    padding-top: 0;
}
.agree .pop-list>li:before {
    display: none;
}
.agree .pop-list>li span{
    font-size: 14px;
