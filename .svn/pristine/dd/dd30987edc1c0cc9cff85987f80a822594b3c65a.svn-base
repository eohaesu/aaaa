/*인풋내 버튼이 필요한 경우*/
.inBtn input {
    position: relative;
}
.inBtn .btn-group {
    position: absolute;
    right: 20px;
    background-color: #fff;
    width: auto;
    margin-top: 0;
    justify-content: flex-end;
    padding: 0;
}

/* 주소 테이블 */
.detail {
    flex-wrap: nowrap;
}
.detail li {
    flex: auto;
}
.detail li span {
    font-size: 14px;
    align-content: center;
}
.table-box {
    width: 100%;
    max-height: 352px;
    overflow: auto;
    overflow-x: hidden;
}
.address .table .thead {
    position: sticky;
    top: 0;
    z-index: 2;
}
.addr {
    width: 40%;
}

/*아코디언 박스*/
.accordion-box {
    border-top: 1px solid #eee;
}
.accordion-header {
    position: relative;
    display: flex;
    align-items: center;
    width: 100%;
    padding: 16px 60px 16px 20px;
    background-color: #fff;
    word-break: keep-all;
    color: #666
}
.accordion-btn {
    position: absolute;
    right: 8px;
    top: 50%;
    transform: translateY(-50%);
    width: 40px;
    height: 40px;
    min-width: unset;
    padding: 0;
    border: 0;
    border-radius: 0;
    background-color: transparent;
}
.toggleIcon {
    flex: none;
    overflow: hidden;
    display: block;
    width: 13px;
    height: 7px;
    position: absolute;
    right: 14px;
    top: 50%;
    text-indent: -9999px;
    transition: all .3s ease;
    background-image: url(../images/common_icon.svg);
    background-size: 500px;
    background-repeat: no-repeat;
    background-position: -250px -20px;
    transform: translateY(-50%) rotate(180deg);
}
.acco-active .text>p {
    font-weight: 500;
    color: #000;
}
.acco-active .toggleIcon {
    transform: translateY(-50%) rotate(0deg);
}
.accordion-body {
    padding: 0 16px 16px;
    font-size: 12px;
    color: #666;
    display: none;
}
.acco-active .accordion-body {
    display: block;
}

/*동의아코디언 체크박스*/
.agreeCheck{
    padding: 16px 60px 16px 20px;
}
.agreeCheck .text p {
    font-size: 13px;
    font-weight: 500;
    word-break: keep-all;
    word-wrap: break-word;
}

/*공지 아코디언 박스*/
.accordion-box.notice {
    background-color: #f5f5f5;
}
.accordion-box.notice .accordion-header {
    background-color: #f5f5f5;
    padding: 16px 60px 16px 48px;
    font-size: 14px;
}
.notice .text {
    position: relative;
}
.notice .text:before {
    content: "";
    position: absolute;
    top: 50%;
    left: -28px;
    transform: translateY(-50%);
    width: 20px;
    height: 20px;
    border-radius: 50%;
    background-image: url(../images/common_icon.svg);
    background-size: 500px;
    background-repeat: no-repeat;
    background-position: -160px -45px;
}





/* 탭 메뉴 */
/*tab css*/
.tab {
    display: flex;
    flex-direction: column;
}
.tab>*:last-child {
    padding-bottom: 4em;
}
.tabmenu-section {
    width: 100%;
    height: 100%;
}
.tapnav-wrap {
    border-bottom: 1px solid #efefef;
}
.tabnav {
    font-size: 0;
    width: 100%;
    overflow-x: auto;
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
}
.tabnav li {
    flex-grow: 1;
    text-align:center;
}
.tabnav li a:before {
    content: "";
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%; 
    height: 2px;
}
.tabnav li a.active:before {
    background:#E6007E;
}
/*.tabnav li a.active {
    border-bottom: 1px solid #fff;
}*/
.tabnav li a {
    position: relative;
   flex-grow: 1;
    display: block;
    background-color: #fff;
    color: #666;
    font-weight: 500;
    padding: 16px 20px;
    font-size:16px;
    white-space:nowrap;
}
.tabnav li a:hover, .tabnav li a.active {
    background:#fff;
    color:#E6007E;
}
.tabcontent {
    height: calc(100% - 50px);
}
.tabcontent>div {
    height: 100%;
}






/* 캘린더 */
input[type="date"] {
    padding: 10px 0 7px 0;
    color: #000;
    font-weight: 400;
}
input[type="date"]::-webkit-inner-spin-button {
    display: none;
    appearance: none;
}
.calendar {
    position: relative;
}
.calendar-icon {
    position: absolute;
    display: block!important;
    right: 0;
    width: 24px;
    height: 24px;
    background-image: url(../images/common_icon.svg);
    background-size: 500px;
    background-repeat: no-repeat;
    background-position: -368px -213px;
}
.datepicker:read-only {
    border-color: #bbb !important;
    color: #000 !important;
}
.datepicker:disabled~.calendar-icon {
    opacity: 0.35;
}
.ui-datepicker.ui-widget.ui-widget-content {
    position: fixed !important;
    width: 100%;
    left: 0 !important;
    top: 30% !important;
    padding: 20px;
    font-size: 15px;
    border: none;
}
.ui-corner-all {
    border-radius: 0;
}
.ui-widget-header {
    background-color: #fff;
    font-size: 14px;
    border: none;
}
.ui-widget-header span {
    font-weight: 600;
}
.ui-datepicker-calendar {
    table-layout: fixed;
    margin: 0;
}
.ui-datepicker-calendar td {
    height: 32px;
}
.ui-state-default, .ui-widget-content .ui-state-default, .ui-widget-header .ui-state-default, .ui-button {
    /* 일자 기본영역 */
    border: none;
    background: none;
    text-align: center;
    width: 32px;
    height: 32px;
    line-height: 32px;
    border-radius: 100%;
    padding:0;
    margin: 0 auto;
    color: #000;
}
.ui-state-highlight, .ui-widget-content .ui-state-highlight, .ui-widget-header .ui-state-highlight{
    /* 오늘일자 */
    border:2px solid #000;
    background:#fff;
    color: #000;
    font-weight: 600;
    line-height: 29px;
}
.ui-state-active, .ui-widget-content .ui-state-active, .ui-widget-header .ui-state-active, a.ui-button:active, .ui-button:active, .ui-button.ui-state-active:hover{
    /* 선택 일자 */
    border:2px solid #e6007e;
    background-color: #e6007e;
    color: #fff;
    font-weight: 600;
    line-height: 29px;
}
.ui-widget-header .ui-icon {
    background-image: unset;
}
.ui-datepicker .ui-datepicker-prev span, .ui-datepicker .ui-datepicker-next span {
    top: 60%;
}
.ui-datepicker-prev span {
    content: "";
    position: relative;
    top: -1px;
    display: inline-block;
    width: 10px;
    height: 10px;
    vertical-align: middle;
    border-color: currentcolor;
    border-style: solid;
    border-width: 2px 0 0 2px;
    border-radius: 1px;
    box-sizing: border-box;
    transform-origin: center;
    transform: rotate(-45deg) scale(.7);
}
.ui-datepicker-next span {
    content: "";
    position: relative;
    top: -1px;
    display: inline-block;
    width: 10px;
    height: 10px;
    vertical-align: middle;
    border-color: currentcolor;
    border-style: solid;
    border-width: 2px 0 0 2px;
    border-radius: 1px;
    box-sizing: border-box;
    transform-origin: center;
    transform: rotate(135deg) scale(.7);
}
.ui-datepicker-calendar .ui-state-disabled {
    opacity: 1;
    background-image: none;
    background-color: #f3f3f3;
}
.ui-datepicker-calendar .ui-state-disabled span {
    color: #888;
}
.ui-datepicker-other-month.ui-datepicker-unselectable {
    opacity: 0;
}




