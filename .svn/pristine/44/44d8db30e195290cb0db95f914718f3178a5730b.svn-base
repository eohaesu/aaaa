.loading {
	position: absolute;
	left: 0;
	right: 0;
	bottom: 0;
	width: 100%;
	height: 100%;
	margin: 0 auto;
	display: flex;
	align-items:center;
	z-index: 1000000;
}

.loading:before {
	width:4em;
	height: 4em;
	content: '';
	display: block;
	background-repeat: no-repeat;
	background-size: 50px;
	background-position: 50% 50%;
	text-align: center;
	margin: 0 auto;
	background-image: url(../images/loading.gif);

}

.bottomspan {
    font-weight: normal;
    font-size: 0.66em;
	width:100%;
	text-align: center;
	position: absolute;
	bottom: 4.5em;
	background: #f9f9f9;
	padding:0.3em 0;
	z-index: 101;
}


.swiper-pagination {
	position: absolute;
	width: 100%;
	margin-top:-1.15em;
	z-index: 100;
}

.swiper-pagination .swiper-pagination-bullet {
    background:#D9D9D9;
    width: 10px;
    height: 10px;
    margin-right: 6px;
    outline: none;
	display: inline-block;
	border-radius: 50%;
}

.swiper-pagination .swiper-pagination-bullet:last-child {
    margin-right: 0;
}

.swiper-pagination .swiper-pagination-bullet-active {
    background:#FFCC0C;
    width: 10px;
    height: 10px;
}

